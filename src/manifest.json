{
    "applications": {
        "gecko": {
            "id": "gout@regseb.github.io",
            "strict_min_version": "53.0"
        }
    },
    "manifest_version": 2,
    "name": "Gout",
    "version": "0.7.1",

    "description": "Agrégateur d'Internet (flux RSS et tout le reste).",
    "icons": {
        "16":  "img/icon-16.png",
        "48":  "img/icon-48.png",
        "128": "img/icon-128.png"
    },

    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "19": "img/icon-19.png",
            "38": "img/icon-38.png"
        },
        "default_title": "Gout",
        "default_popup": "popup/index.html"
    },

    "author": "Sébastien Règne <regseb@gmail.com>",

    "homepage_url": "https://github.com/regseb/gout/",
    "key": "",
    "minimum_chrome_version": "58",
    "permissions": ["<all_urls>", "history", "identity"]
}
